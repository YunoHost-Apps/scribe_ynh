[Unit]
Description=DPM instance for Scribe punctuation feature
After=network.target

[Service]
User=$app
Group=scribe
WorkingDirectory=$install_dir/punctuation-server
ExecStart=. venv/bin/activate && DMP_SERVER_INTERFACE=0.0.0.0 DMP_SERVER_PORT=2800 python3 server.py
[Install]
WantedBy=multi-user.target
